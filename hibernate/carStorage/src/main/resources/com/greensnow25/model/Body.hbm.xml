<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greensnow25.model.Body" table="body_car">
        <id name="id" type="int" column="id_car_body">
            <generator class="native"/>
        </id>
        <property name="name" column="name_body" type="string"/>

        <set name="cars" inverse="true">
            <key column="id_car_body"/>
            <one-to-many class="com.greensnow25.model.Car"/>
        </set>

    </class>
</hibernate-mapping>